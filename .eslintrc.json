{
    "extends": [ "semistandard", "plugin:react/recommended" ],
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "script"
    },
    "rules": {
        "no-multi-spaces": "off",
        "key-spacing": "off",
        "standard/array-bracket-even-spacing": "off",
        "camelcase": "off",

        "react/prop-types": "off"
    }
}